{
  "global": {
    "lang": "ja",
    "title": "example site",
    "keywords": "static site generator,NodeJS,React",
    "copyright": "@Himenon",
    "viewport": {
      "initial-scale": 1.0,
      "minimum-scale": 0.5,
      "user-scalable": "no"
    },
    "thirdParty": {
      "googleAnalytics": {
        "ua": "test"
      }
    },
    "js": [
      "/assets/main.js"
    ],
    "css": [
      "/assets/main.css",
      "https://fonts.googleapis.com/css?family=Open+Sans:600,800"
    ]
  },
  "source": "./src",
  "baseUrl": "http://localhost:9001",
  "baseUri": "/sample",
  "layoutFile": "./template/template.js",
  "customComponentsFile": "./components/components.js",
  "port": 9000,
  "plugins": [
    {
      "name": "MetaPlugin",
      "resolve": "./plugins"
    }
  ],
  "blacklist": {
    "extensions": [".html", ".json", ".mdx", ".ts", ".tsx"]
  }
}
