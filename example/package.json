{
  "name": "example",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "start": "run-p *:watch",
    "develop": "chmod +x ../lib/cli.js && node ../lib/cli.js . -Do",
    "develop2": "chmod +x ../lib/cli.js && node ../lib/cli.js --config ./src/config.json -Do",
    "build": "chmod +x ../lib/cli.js && node ../lib/cli.js . --out-dir hoge",
    "build2": "chmod +x ../lib/cli.js && node ../lib/cli.js --config ./src/config.json --out-dir hoge",
    "components": "tsc ./CustomComponent/components.tsx --jsx react --lib esnext --target esnext --module commonjs",
    "components:watch": "tsc -w ./CustomComponent/components.tsx --jsx react --lib esnext --target esnext --module commonjs",
    "layout": "tsc ./CustomTemplate/template.tsx --jsx react --lib esnext --target esnext --module commonjs",
    "layout:watch": "tsc -w ./CustomTemplate/template.tsx --jsx react --lib esnext --target esnext --module commonjs"
  },
  "dependencies": {
    "@custom-site/custom-site": "file:../"
  },
  "devDependencies": {
    "npm-run-all": "^4.1.5",
    "typescript": "^3.2.4"
  }
}
