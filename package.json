{
  "name": "@custom-site/custom-site",
  "version": "0.1.1",
  "description": "Static Site Generator",
  "keywords": [
    "template",
    "typescript"
  ],
  "homepage": "https://github.com/custom-site/custom-site#readme",
  "bugs": {
    "url": "https://github.com/custom-site/custom-site/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/custom-site/custom-site.git"
  },
  "license": "MIT",
  "author": "Himenon",
  "files": [
    "lib",
    "typings",
    "package.json"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "bin": {
    "custom-site": "./lib/cli.js"
  },
  "directories": {
    "lib": "lib",
    "example": "example"
  },
  "scripts": {
    "build": "yarn run clean && tsc -p ./tsconfig.build.json",
    "build:watch": "yarn run build --watch",
    "ci:notify:release": "ts-node ./scripts/notify.ts",
    "clean": "rimraf ./lib",
    "cli": "node ./lib/cli.js",
    "cli:build": "node ./lib/cli.js example --out-dir out",
    "commitmsg": "commitlint -e $GIT_PARAMS",
    "components": "tsc ./CustomComponent/components.tsx --jsx react --lib esnext --target esnext --module commonjs",
    "components:watch": "tsc -w ./CustomComponent/components.tsx --jsx react --lib esnext --target esnext --module commonjs",
    "cruiser:image": "depcruise  --exclude \"^node_modules\" --output-type dot src/server | dot -T png > docs/dependencygraph.png",
    "develop": "yarn run build --watch",
    "develop:server": "chmod +x ./lib/cli.js && ./lib/cli.js example -Do",
    "format": "prettier --config .prettierrc --write src/*.{ts,tsx}",
    "layout": "tsc ./CustomTemplate/template.tsx --jsx react --lib esnext --target esnext --module commonjs",
    "layout:watch": "tsc -w ./CustomTemplate/template.tsx --jsx react --lib esnext --target esnext --module commonjs",
    "lint": "tslint -c tslint.json -p tsconfig.json",
    "lint:fix": "yarn run lint --fix",
    "prettier": "prettier --config .prettierrc --write src/*.{ts,tsx}",
    "start": "run-p *:watch",
    "test": "yarn run test:cruiser && yarn run test:jest",
    "test:ci": "yarn run test && codecov",
    "test:cruiser": "dependency-cruiser --validate .dependency-cruiser.json src",
    "test:jest": "jest -c ./jest.config.json",
    "test:watch": "yarn run test:jest --watch",
    "version_up:major": "changelog -M && git add CHANGELOG.md && git commit -m \"chore(change-log): updated CHANGELOG.md\" && npm version major",
    "version_up:minor": "changelog -m && git add CHANGELOG.md && git commit -m \"chore(change-log): updated CHANGELOG.md\" && npm version minor",
    "version_up:patch": "changelog -p && git add CHANGELOG.md && git commit -m \"chore(change-log): updated CHANGELOG.md\" && npm version patch"
  },
  "dependencies": {
    "@babel/core": "^7.1.6",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/plugin-transform-react-jsx": "^7.1.6",
    "@babel/standalone": "^7.1.0",
    "@mdx-js/mdx": "^0.16.0",
    "@mdx-js/tag": "^0.16.0",
    "chalk": "^2.4.1",
    "chokidar": "^2.0.4",
    "dot-prop": "^4.2.0",
    "fs-extra": "^7.0.1",
    "gray-matter": "^4.0.1",
    "load-json-file": "^5.1.0",
    "meow": "^5.0.0",
    "mime-types": "^2.1.21",
    "opn": "^5.4.0",
    "portfinder": "^1.0.19",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "read-pkg": "^4.0.1",
    "read-pkg-up": "^4.0.0",
    "recursive-readdir": "^2.2.2",
    "update-notifier": "^2.5.0",
    "ws": "^6.1.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^7.2.1",
    "@commitlint/config-conventional": "^7.1.2",
    "@slack/client": "^4.8.0",
    "@types/chokidar": "^1.7.5",
    "@types/dot-prop": "^4.2.0",
    "@types/fs-extra": "^5.0.4",
    "@types/jest": "^23.3.1",
    "@types/load-json-file": "^2.0.7",
    "@types/meow": "^5.0.0",
    "@types/mime-types": "^2.1.0",
    "@types/node": "^10.12.9",
    "@types/opn": "^5.1.0",
    "@types/react": "^16.7.6",
    "@types/react-dom": "^16.0.9",
    "@types/read-pkg-up": "^3.0.1",
    "@types/recursive-readdir": "^2.2.0",
    "@types/update-notifier": "^2.5.0",
    "codecov": "^3.1.0",
    "dependency-cruiser": "^4.8.0",
    "generate-changelog": "^1.7.1",
    "husky": "^1.1.3",
    "jest": "^23.5.0",
    "jest-cli": "^23.5.0",
    "lint-staged": "^8.0.4",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.14.2",
    "rimraf": "^2.6.2",
    "sort-package-json": "^1.16.0",
    "ts-jest": "^23.1.3",
    "ts-node": "^7.0.1",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.15.0",
    "tslint-config-standard": "^8.0.1",
    "tslint-plugin-prettier": "^2.0.1",
    "typescript": "^3.2.2",
    "yarn-outdated-notifier": "^1.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
